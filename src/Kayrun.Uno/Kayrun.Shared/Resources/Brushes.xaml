<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <ResourceDictionary.ThemeDictionaries>
        <ResourceDictionary x:Key="Dark">
            <SolidColorBrush x:Key="PrimaryBackground" Color="#0d0d0c"/>
            <SolidColorBrush x:Key="SecondaryBackground" Color="#151413"/>
            <SolidColorBrush x:Key="TertiaryBackground" Color="#1f1e1e"/>
            <SolidColorBrush x:Key="InvertedBackground" Color="#e1e3e5"/>
        </ResourceDictionary>
        <ResourceDictionary x:Key="Light">
            <SolidColorBrush x:Key="PrimaryBackground" Color="#e1e3e5"/>
            <SolidColorBrush x:Key="SecondaryBackground" Color="#e8e5e3"/>
            <SolidColorBrush x:Key="TertiaryBackground" Color="#f4f2f1"/>
            <SolidColorBrush x:Key="InvertedBackground" Color="#1c1917"/>
        </ResourceDictionary>
    </ResourceDictionary.ThemeDictionaries>

</ResourceDictionary>
